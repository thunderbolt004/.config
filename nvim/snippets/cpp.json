{
    "CP template": {
        "prefix": "cp",
        "body": [
            "// libraries",
            "#include <iostream>",
            "#include <vector>",
            "// optimizations",
            "#pragma GCC optimize(\"O2\")",
            "// definitions",
            "#define ll long long",
            "#define DIV 1000000007",
            "#define all(x) x.begin(), x.end()",
            "#define inp(v,i,n) for(int i = 0; i < n; i++) std::cin >> v[i];",
            "// vector output",
            "// for input, os->is & cout->cin",
            "template<typename T>",
            "std::ostream& operator<<(std::ostream& os, std::vector<T>& intermediate_array) { for (auto& a : intermediate_array) std::cout << a << ' '; return os; }",
            "// typedefs",
            "typedef std::vector<int> vi;",
            "typedef std::vector<ll> vl;",
            "typedef std::pair<int, int> pi;",
            "typedef std::pair<ll, ll> pl;",
            "",
            "void solve () {",
            "\tint n$1;",
            "\tstd::cin >> n$2;",
            "\tvi v(n);",
            "\tinp(v,i,n);",
            "\t$3",
            "}",
            "",
            "int main () {",
            "\t// good for large in/out",
            "\tstd::ios_base::sync_with_stdio(false);",
            "\tstd::cin.tie(nullptr);",
            "",
            "\tint cases;",
            "\tstd::cin>>cases;",
            "\twhile (cases--) {",
            "\t\tsolve();",
            "\t}",
            "\treturn 0;",
            "}"
        ],
        "description": "A standard CP template"
    }
}
